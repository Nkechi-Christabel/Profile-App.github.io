\n\n//set currentpage \nconst handleClick = (e) => {\n      console.log(e)\n      setCurrentPage(e)\n}\n \n\n  return (\n    <div className=\"App\">\n       <Main handleChange={handleChange} inputValue={searchInput} handleGenderSelect={handleGenderSelect} genderValue={genderFilter} handlePaymentSelect={handlePaymentSelect} paymentValue={paymentFilter} />\n        <div className=\"profile__details-container\">\n       { isLoading ? <Loader/ > :\n           currentProfile.filter( person => { \n            const name = person.FirstName + ' ' + person.LastName; \n            return name.toLowerCase().includes(searchInput);\n           }).map((person, index) => <Profile key={index} user={person} />)\n          }\n        </div>\n      \n        <div className=\"paginationButtons\">\n       {\n         NumberOfPagesArr.map( number =>  <button className={number === currentPage ? 'active' : ''} onClick={ () => handleClick(number)}>{number}</button>)\n       }\n        </div>\n    </div>\n  )\n}\nexport default App;\n\n\n\n\n","C:\\Users\\Promh\\Desktop\\enye-frontend-app\\enye-frontend-app\\src\\reportWebVitals.js",[],"C:\\Users\\Promh\\Desktop\\enye-frontend-app\\enye-frontend-app\\src\\Components\\Main.js",[],"C:\\Users\\Promh\\Desktop\\enye-frontend-app\\enye-frontend-app\\src\\Components\\Profile.js",[],"C:\\Users\\Promh\\Desktop\\enye-frontend-app\\enye-frontend-app\\src\\Components\\Loader.js",[],["33","34"],{"ruleId":"35","replacedBy":"36"},{"ruleId":"37","replacedBy":"38"},{"ruleId":"39","severity":1,"message":"40","line":11,"column":27,"nodeType":"41","messageId":"42","endLine":11,"endColumn":42},{"ruleId":"39","severity":1,"message":"43","line":12,"column":28,"nodeType":"41","messageId":"42","endLine":12,"endColumn":44},{"ruleId":"35","replacedBy":"44"},{"ruleId":"37","replacedBy":"45"},"no-native-reassign",["46"],"no-negated-in-lhs",["47"],"no-unused-vars","'setGenderFilter' is assigned a value but never used.","Identifier","unusedVar","'setPaymentFilter' is assi